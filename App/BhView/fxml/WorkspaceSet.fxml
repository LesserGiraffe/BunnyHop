<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane
  xmlns:fx="http://javafx.com/fxml"
  fx:id="workspaceSetViewBase"
  fx:controller="net.seapanda.bunnyhop.workspace.control.WorkspaceSetController">
  <TabPane fx:id="workspaceSetTab" tabClosingPolicy="ALL_TABS" tabDragPolicy="REORDER"/>
  <fx:include fx:id="trashCan" source="TrashCan.fxml" MouseTransparent="true"/>
  <fx:include
     fx:id="searchBox"
     source="SearchBox.fxml"
     visible="false"
     StackPane.Alignment="BOTTOM_LEFT"/>
</StackPane>
