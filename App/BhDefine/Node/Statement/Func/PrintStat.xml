<Node
  type="connective"
  bhNodeID="idPrintStat"
  name="PrintStat"
  nodeStyleID="PrintStat.json"
  breakpoint="set"
  onMovedFromChildToWS="OnStatMovedToWS.js"
  onMovedFromWsToChild="OnStatMovedToChild.js"
  onDeletionRequested="OnDeletionRequested.js"
  onCutRequested="OnCutRequested.js"
  onCopyRequested="OnCopyRequested.js"
  onDragStarted="OnStatDragStarted.js"
  onAliasAsked="OnAliasAsked.js"
  version="bhn-1.0.0">

  <ConnectorSection name="StatSctn">
    
    <Connector
      bhConnectorID="idPrintStat.cnctr0"
      import="idAnyExpCnctr"
      name="Arg0"/>

    <Connector
     bhConnectorID="idPrintStat.cnctr1"
     name="Option0"
     fixed="true">
      <Node
        type="text"
        bhNodeID="idPrintStat.node1"
        name="lineFeedOption"
        nodeStyleID="WordComboBox.json"
        breakpoint="specifyParent"
        onTextOptionsCreating="CreateLineBreakOps.js"
        version="bhn-1.0.0">
      </Node>
    </Connector>

    <Connector
      bhConnectorID="idPrintStat.cnctr2"
      import="idStatCnctr"
      name="NextStat"/>

  </ConnectorSection>
</Node>
