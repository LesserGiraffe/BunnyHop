  onMovedFromWsToChild="OnStatMovedToChild.js"
  onDeletionRequested="OnStatOrDeclDeletionRequested.js"
  onCutRequested="OnCutRequested.js"
  onCopyRequested="OnCopyRequested.js"
  onUiEventReceived="OnUiEventReceivedByStat.js"
  onAliasAsked="OnAliasAsked.js"
  version="bhn-0.1.0">

  <ConnectorSection name="Statement">
    
    <Connector
      bhConnectorID="idIfStat.cnctr0"
      import="idBoolExpCnctr"
      defaultBhNodeID="idBoolExpVoid"
      name="CondExp"/>

    <Connector
      bhConnectorID="idIfStat.cnctr1"
      import="idStatCnctr"
      name="ThenStat"/>
    
    <Connector
      bhConnectorID="idIfStat.cnctr2"
      import="idStatCnctr"
      name="NextStat"/>

  </ConnectorSection>
</Node>
