<Node
  type="connective"
  bhNodeID="idVoidFuncDef"
  name="VoidFuncDef"
  nodeStyleID="VoidFuncDef.json"
  onPrivateTemplateCreating="CreateVoidFuncPrivateTemplate.js">

  <Imitation imitationID="imitIdFuncCall" imitationNodeID="idVoidFuncCall"/>

  <Section name="FuncDefSctn">
    
    <ConnectorSection name="FuncNameSctn">
      <Connector
        bhConnectorID="idFuncNameCnctr"
        name="FuncName"
        imitationID="imitIdFuncName"
        imitCnctPos="imitPosFuncName"/>
    </ConnectorSection>
    
    <ConnectorSection name="VarScopeSctn">
      <Connector bhConnectorID="idStatCnctr" name="StatList"/>
    
      <Connector
        bhConnectorID="idVarDeclCnctr"
        name="ParamDecl"
        imitationID="imitIdVarDeclToArg"
        imitCnctPos="imitPosArg"/>
    
      <Connector
        bhConnectorID="idVarDeclCnctr"
        name="OutParamDecl"
        imitationID="imitIdVarDeclToOutArg"
        imitCnctPos="imitPosOutArg"/>
    </ConnectorSection>
  </Section>
</Node>
