<Node
	type="connective"
	bhNodeID="idReusableBarrierDecl"
	name="ReusableBarrierDecl"
	canCreateImitManually="true"
	nodeStyleID="ReusableBarrierDecl.json"
	onMovedFromChildToWS="onGlobalDataDeclMovedToWS.js"
	onDeletionRequested="onDeletionRequested.js"
	onCutRequested="reconnectOuter.js">

	<Imitation imitationID="imitIdManual" imitationNodeID="idReusableBarrierVar"/>

	<ConnectorSection name="GlobalDataDeclSctn">

		<PrivateConnector
			bhConnectorID="idReusableBarrierDecl.prvCnctr0"
			name="machiawase"
			fixed="true">
			<Node
				type="label"
				bhNodeID="idReusableBarrierDecl.prvNode0"
				name="machiawase"
				nodeStyleID="WordLabel2.json"
				nodeInputControl="Label.fxml"
				initString="同期地点">
			</Node>
		</PrivateConnector>

		<Connector
			bhConnectorID="idVarNameCnctr"
			name="ReusableBarrierName"
			imitationID="imitIdVarName"
			imitCnctPos="imitPosVarName"/>

		<PrivateConnector
			bhConnectorID="idReusableBarrierDecl.prvCnctr1"
			name="matsuShoriNoKazu"
			fixed="true">
			<Node
				type="label"
				bhNodeID="idReusableBarrierDecl.prvNode1"
				name="matsuShoriNoKazu"
				nodeStyleID="WordLabel2.json"
				nodeInputControl="Label.fxml"
				initString="待つ処理の数">
			</Node>
		</PrivateConnector>

		<PrivateConnector
			bhConnectorID="idReusableBarrierDecl.prvCnctr2"
			name="Arg0"
    		fixed="true">
			<Node
				type="textField"
				bhNodeID="idReusableBarrierDecl.prvNode2"
				name="NumLiteral"
				nodeStyleID="FixedNumLiteral.json"
				nodeInputControl="TextField.fxml"
				textFormatter="FullWidthToHalfWidth.js"
				textAcceptabilityChecker="CheckPositiveIntFormat.js"
				initString="2">
			</Node>
		</PrivateConnector>

		<Connector bhConnectorID="idGlobalDataDeclCnctr" name="NextGlobalDataDecl"/>
	</ConnectorSection>
</Node>
