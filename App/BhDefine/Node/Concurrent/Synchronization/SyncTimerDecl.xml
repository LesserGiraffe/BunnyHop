			</Node>
		</PrivateConnector>

		<PrivateConnector
			bhConnectorID="idSyncTimerDecl.prvCnctr4"
			name="Arg0"
    		fixed="true">
			<Node
				type="text"
				bhNodeID="idSyncTimerDecl.prvNode4"
				name="NumLiteral"
				nodeStyleID="FixedNumLiteral.json"
				onTextFormatting="FullWidthToHalfWidth.js"
