<Node
  type="connective"
  bhNodeID="idSemaphoreDecl"
  name="SemaphoreDecl"
  nodeStyleID="SemaphoreDecl.json"
  breakpoint="ignore"
  onCompanionNodesCreating="CreateSemaphoreCompanions.js"
  onMovedFromChildToWS="OnGlobalDataDeclMovedToWS.js"
  onMovedFromWsToChild="OnGlobalDataDeclMovedToChild.js"
  onDeletionRequested="OnDeletionRequested.js"
  onCutRequested="OnCutRequested.js"
  onCopyRequested="OnCopyRequested.js"
  onRelatedNodesRequired="GetDerivationFamily.js"
  onUiEventReceived="OnUiEventReceivedByGlobalDataDecl.js"
  onAliasAsked="OnAliasAsked.js"
  onUserDefinedNameAsked="OnUserDefinedNameAsked.js"
  version="bhn-0.1.0">

  <Derivation derivationID="dervIdSemaphoreVar" derivativeID="idSemaphoreVar"/>

  <ConnectorSection name="GlobalDataDecl">

    <Connector
      bhConnectorID="idSemaphoreDecl.cnctr0"
      import="idIdentifierNameCnctr"
      name="SemaphoreName"
      derivationID="dervIdIdentifierName"
      derivativeJoint="dervJointVarName"/>

    <Connector
      bhConnectorID="idSemaphoreDecl.cnctr1"
      name="Arg1"
      fixed="true">
      <Node
        type="text"
        bhNodeID="idSemaphoreDecl.node1"
        name="BoolLiteral"
        nodeStyleID="WordComboBox.json"
        breakpoint="specifyParent"
        onTextOptionsCreating="CreateSemaphoreOps.js"
        version="bhn-0.1.0"/>
    </Connector>

    <Connector
      bhConnectorID="idSemaphoreDecl.cnctr2"
      name="Arg0"
      fixed="true">
      <Node
        type="text"
        bhNodeID="idSemaphoreDecl.node2"
        name="NumLiteral"
        nodeStyleID="FixedNumLiteral.json"
        breakpoint="specifyParent"
        onTextFormatting="FullWidthToHalfWidth.js"
        onTextChecking="CheckSemaphorePermit.js"
        text="1"
        version="bhn-0.1.0"/>
    </Connector>

    <Connector
      bhConnectorID="idSemaphoreDecl.cnctr3"
      import="idGlobalDataDeclCnctr"
      name="NextGlobalDataDecl"/>

  </ConnectorSection>
</Node>
