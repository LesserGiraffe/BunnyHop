<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import net.seapanda.bunnyhop.view.*?>

<ComboBox xmlns:fx="http://javafx.com/fxml">
	<items>
		<FXCollections fx:factory="observableArrayList">
			<SelectableItem fx:id="idMoveForward" modelText="moveForward" viewText="前進"/>
			<SelectableItem fx:id="idMoveBackward" modelText="moveBackward" viewText="後退"/>
			<SelectableItem fx:id="idTurnRight" modelText="turnRight" viewText="右旋回"/>
			<SelectableItem fx:id="idTurnLeft" modelText="turnLeft" viewText="左旋回"/>
		</FXCollections>
	</items>
</ComboBox>