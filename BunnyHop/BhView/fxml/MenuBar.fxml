<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.*?>

<MenuBar 
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="net.seapanda.bunnyhop.control.MenuBarController"
	fx:id="menuBar">
	<menus>
		<Menu text="ファイル">
			<items>
				<MenuItem fx:id="loadMenu" text="ファイルを開く"/>
				<MenuItem fx:id="saveMenu" text="上書き保存"/>
				<MenuItem fx:id="saveAsMenu" text="名前を付けて保存"/>
			</items>
		</Menu>
		<Menu text="ヘルプ">
			<items>
				<MenuItem fx:id="aboutBunnyHop" text="BunnyHopについて"/>
			</items>
		</Menu>
	</menus>
</MenuBar>
